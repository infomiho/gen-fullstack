{
  "name": "@gen-fullstack/server",
  "version": "1.0.0",
  "type": "module",
  "packageManager": "pnpm@9.15.5",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
