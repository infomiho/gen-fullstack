    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "prettier --check \"src/**/*.ts\""
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.52",
    "@gen-fullstack/shared": "workspace:*",
    "ai": "^5.0.68",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^5.0.1",
    "openai": "^4.78.3",
    "socket.io": "^4.8.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.2",
    "@vitest/coverage-v8": "^3.1.5",
    "@vitest/ui": "^3.1.5",
    "prettier": "^3.4.2",
    "tsx": "^4.19.3",
    "typescript": "^5.9.0",
    "vitest": "^3.1.5"
  }
