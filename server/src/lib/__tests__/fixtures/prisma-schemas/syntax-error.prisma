// Schema with syntax errors
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id    Int    @id @default(autoincrement())
  name  String
  email String @unique
  // Missing closing brace

model Post {
  id    Int    @id @default(autoincrement())
  title String
}
