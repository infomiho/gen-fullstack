// Schema with missing relation field
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id    Int    @id @default(autoincrement())
  name  String
  posts Post[] // Relation defined here
}

model Post {
  id      Int    @id @default(autoincrement())
  title   String
  // Missing userId field and User relation
}
