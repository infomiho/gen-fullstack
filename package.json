{
  "name": "gen-fullstack",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@9.15.5",
  "scripts": {
    "dev": "nx run-many -t dev --projects=client,server --output-style=stream",
    "dev:server": "nx run server:dev",
    "dev:client": "nx run client:dev",
    "build": "nx run-many -t build",
    "test": "nx run-many -t test:run",
    "test:watch": "nx run-many -t test --parallel=3",
    "test:ui": "nx run-many -t test:ui --parallel=3",
    "typecheck": "nx run-many -t typecheck",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "check": "biome check --write .",
    "clean": "pnpm -r clean && rm -rf node_modules .nx",
    "prepare": "husky"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.6",
    "husky": "^9.1.7",
    "nx": "^22.0.2",
    "typescript": "^5.9.0"
  },
  "dependencies": {
    "@types/dockerode": "^3.3.44",
    "@types/http-proxy": "^1.17.16",
    "dockerode": "^4.0.9",
    "http-proxy": "^1.18.1",
    "tree-kill": "^1.2.2"
  }
}
